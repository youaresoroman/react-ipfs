{
  "version": "0.3.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "start:examples": "cd examples && npm start",
    "install:examples": "cd examples && npm install",
    "documentation": "npx typedoc"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-ipfs",
  "author": "Roman Yankowski",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youaresoroman/react-ipfs.git"
  },
  "keywords": [
    "React",
    "Component",
    "Library",
    "Rollup",
    "Typescript",
    "User Interface",
    "Storybook",
    "IPFS"
  ],
  "bugs": {
    "url": "https://github.com/youaresoroman/react-ipfs/issues"
  },
  "homepage": "https://github.com/youaresoroman/react-ipfs#readme",
  "module": "dist/react-ipfs.esm.js",
  "size-limit": [
    {
      "path": "dist/react-ipfs.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-ipfs.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^5.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "gulp": "^4.0.2",
    "ipfs": "^0.60.2",
    "react-bootstrap": "^2.0.3",
    "uint8arrays": "^3.0.0"
  }
}
